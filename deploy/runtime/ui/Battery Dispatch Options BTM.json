        "67": "",
        "68": "\t}",
        "69": "\telse",
        "70": "\t{",
        "71": "\t\toption = value('batt_dispatch_choice_ui');",
        "72": "\t\tenable('batt_dispatch_auto_can_charge',option != 3);",
        "73": "\t\tenable('batt_dispatch_auto_can_gridcharge', option !=3);",
        "74": "\t\tenable('batt_dispatch_auto_btm_can_discharge_to_grid', option == 2);",
