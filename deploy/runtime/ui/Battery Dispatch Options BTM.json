        "47": "}",
        "48": "",
        "49": "// hide charge options for manual dispatch",
        "50": "function show_hide_charge_options()",
        "51": "{",
        "52": "\tif ( technology() == 'Fuel Cell' && value('en_batt') == 0  )",
        "53": "\t{ ",
        "54": "\t\tenable('batt_dispatch_choice_ui',false); ",
        "55": "\t\t//Peak Shaving Inputs",
        "56": "\t\tenable('peak_shaving_batt_dispatch_choice', false);",
        "57": "\t\t//Input Grid Power Targets Inputs",
        "58": "\t\tenable('batt_target_choice', false);",
        "59": "\t\tenable('batt_target_power', false);",
        "60": "\t\tenable('batt_target_power_monthly', false);",
        "61": "\t\t//Input Battery Power Targets Inputs",
        "62": "\t\tenable('batt_custom_dispatch', false);",
        "63": "\t\t//Price Signal Dispatch Inputs",
        "64": "\t\tenable('batt_cycle_cost', false);",
        "65": "\t\tenable('batt_cycle_cost_choice', false);",
