        "80": "function enable_disable_powerflow_options()",
        "81": "{",
        "82": "\t// enable exceeds load exceeds output option only for AC-connected batteries",
        "83": "\t// for DC-connected batteries disable and set to false",
        "84": "\tif ( technology() == 'Fuel Cell' && value('en_batt') == 0 )",
        "85": "\t{ ",
        "86": "\t\tenable('batt_dispatch_charge_only_system_exceeds_load', false);",
        "87": "\t\tenable('batt_dispatch_discharge_only_load_exceeds_system', false);",
        "88": "\t}",
        "89": "\telse if (value('batt_ac_or_dc') == 0) { // DC-connected",
