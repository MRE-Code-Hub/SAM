    "Equations": [
        "equations{ 'system_capacity' } = define() {\r",
        "\treturn ${ui_pv_capacity} \r",
        "\t+ ${ui_wind_system_capacity} \r",
        "\t+ ${ui_battery_system_capacity}; \r",
        "};\r",
        "\t\t\r",
        "equations{ 'ui_pv_capacity' } = define() {\r",
        "\tif (strpos(lower(technology()), 'pvwatts') > - 1)\r",
        "\t{\r",
        "\t\treturn ${pv_system_capacity} / ${dc_ac_ratio};\r",
        "\t}\r",
        "\telse\r",
        "\t{\r",
        "\t\treturn ${total_inverter_capacity};\r",
        "\t\t/*return ?${inverter_model} \r",
        "\t\t\t[ ${inv_snl_paco},\r",
        "\t\t\t${inv_ds_paco}, \r",
        "\t\t\t${inv_pd_paco}, \r",
        "\t\t\t${inv_cec_cg_paco} ] \r",
        "\t\t\t* ${inverter_count} /1000;*/\r",
        "\t}\t\r",
        "};\r",
        "\r",
        "equations{ 'ui_wind_capacity' } = define() {\r",
        "\treturn ${wind_system_capacity};\r",
        "};\r",
        "\r",
        "equations{ 'ui_battery_capacity' } = define() {\r",
        "\treturn ${battery_system_capacity};\r",
        "};"
