    "Equations": {
        "1": "equations{ '$MIMO$ fap_analysis_period' } = define() {",
        "2": "\tif (financing() == 'Merchant Plant') {",
        "3": "//\tmsgbox('FAP eqns financing, old ' + financing() + ', ' + analysis_period_old());",
        "4": "\t\tif ((analysis_period_old()>= 1) && (${analysis_period} != analysis_period_old())) {",
        "5": "//\t\t\tmsgbox('FAP updating new,old ' + ${analysis_period} + ', ' + analysis_period_old());",
        "6": "\t\t\tdata.analysis_period =  ${analysis_period};",
        "7": "\t\t\tdata.analysis_period_old =  analysis_period_old();",
        "8": "\t\t\tdata.mp_energy_market_revenue =  ${mp_energy_market_revenue};",
        "9": "\t\t\tdata.mp_ancserv1_revenue =  ${mp_ancserv1_revenue};",
        "10": "\t\t\tdata.mp_ancserv2_revenue =  ${mp_ancserv2_revenue};",
        "11": "\t\t\tdata.mp_ancserv3_revenue =  ${mp_ancserv3_revenue};",
        "12": "\t\t\tdata.mp_ancserv4_revenue =  ${mp_ancserv4_revenue};",
        "13": "\t\t\tdata.mp_energy_market_revenue_single =  ${mp_energy_market_revenue_single};",
        "14": "\t\t\tdata.mp_ancserv1_revenue_single =  ${mp_ancserv1_revenue_single};",
        "15": "\t\t\tdata.mp_ancserv2_revenue_single =  ${mp_ancserv2_revenue_single};",
        "16": "\t\t\tdata.mp_ancserv3_revenue_single =  ${mp_ancserv3_revenue_single};",
        "17": "\t\t\tdata.mp_ancserv4_revenue_single =  ${mp_ancserv4_revenue_single};",
        "18": "",
        "19": "\t\t\tsuccess = ssc_eqn('analysisperiodchange', data);",
        "20": "\t\t\t",
        "21": "\t\t\tif (!success){",
        "22": "\t\t\t\tmsgbox('success ' + success);",
        "23": "\t\t\t\treturn 0;",
        "24": "\t\t\t}",
        "25": "\t\t\telse {",
        "26": "\t\t\t\t// need intermediate variable to update",
        "27": "\t\t\t\t${mp_energy_market_revenue_ui} = data.mp_energy_market_revenue;",
        "28": "\t\t\t\t${mp_ancserv1_revenue_ui} = data.mp_ancserv1_revenue;",
        "29": "\t\t\t\t${mp_ancserv2_revenue_ui} = data.mp_ancserv2_revenue;",
        "30": "\t\t\t\t${mp_ancserv3_revenue_ui} = data.mp_ancserv3_revenue;",
        "31": "\t\t\t\t${mp_ancserv4_revenue_ui} = data.mp_ancserv4_revenue;",
        "32": "\t\t\t\t${mp_energy_market_revenue_single_ui} = data.mp_energy_market_revenue_single;",
        "33": "\t\t\t\t${mp_ancserv1_revenue_single_ui} = data.mp_ancserv1_revenue_single;",
        "34": "\t\t\t\t${mp_ancserv2_revenue_single_ui} = data.mp_ancserv2_revenue_single;",
        "35": "\t\t\t\t${mp_ancserv3_revenue_single_ui} = data.mp_ancserv3_revenue_single;",
        "36": "\t\t\t\t${mp_ancserv4_revenue_single_ui} = data.mp_ancserv4_revenue_single;",
        "37": "\t\t\t}",
        "38": "\t\t\t",
        "39": "\t\t\tif (data.analysisperiodchange_check == 0)",
        "40": "\t\t\t{",
        "41": "\t\t\t\tmsg = data.analysisperiodchange_error;",
        "42": "\t\t\t\tmsgbox(msg);",
        "43": "\t\t\t}",
        "44": "\t\t}",
        "45": "\t\t",
        "46": "\t}",
        "47": "\treturn 0;",
        "48": "};",
        "49": "",
        "50": "equations{ 'nominal_discount_rate' } = define() {",
        "51": "\treturn ( (1.0 + ${real_discount_rate}/100.0) * (1.0 + ${inflation_rate}/100.0) - 1.0 ) * 100.0; ",
        "52": "};",
        "53": "",
        "54": ""
    },
    "Callbacks": {
        "1": "on_load{'Financial Analysis Parameters'} = define() {",
        "2": "\ttxt = analysis_period_message();",
        "3": "\tproperty('analysis_period_warning','caption',txt);",
        "4": "};",
        "5": "",
        "6": "function analysis_period_message() {",
        "7": "  fin = financing();",
        "8": "  debt = true;",
        "9": "  msg = '';",
        "10": "  if ( fin == 'All Equity Partnership Flip' || fin == 'Single Owner' || fin == 'Sale Leaseback' || fin == 'Leveraged Parntership Flip' ) {",
        "11": "\tif ( value('analysis_period') < value('flip_target_year') )",
        "12": "\t\tmsg = 'Analysis period is less than IRR target year. ';",
        "13": "  }",
        "14": "  if ( fin == 'All Equity Partnership Flip' || fin == 'Sale Leaseback' ) {",
        "15": "    debt = false;",
        "16": "    if ( fin == 'All Equity Partnership Flip' ) {",
        "17": "\t\tif ( value('analysis_period') < value('sponsor_cap_recovery_year') )",
        "18": "\t\t\tmsg = msg + 'Analysis period is less than capital recovery period. ';",
        "19": "\t}\t",
        "20": "  }",
        "21": "  if ( debt == true ) {",
        "22": "    if ( fin == 'Residential' || fin == 'Commercial' ) {",
        "23": "      if ( value('loan_term') > value('analysis_period') && value('debt_fraction') > 0 )",
        "24": "        msg = 'Analysis period is shorter than the debt payment period (loan term). The cash flow will not account for debt liability beyond the analysis period. ';",
        "25": "    }  ",
        "26": "    else {",
        "27": "      ppa_debt = (value('debt_option') == 1 && value('dscr') > 0) || (value('debt_option')==0 && value('debt_percent')) > 0;",
        "28": "      if ( value('term_tenor') > value('analysis_period') && ppa_debt==true )",
        "29": "        msg = 'Analysis period is shorter than the debt payment period (tenor). The cash flow will not account for debt liability beyond the analysis period. ';",
        "30": "      if (fin != 'Merchant Plant' && fin != 'Community Solar')  ",
        "31": "      {",
        "32": "\t\tif ( value('flip_target_year') > value('analysis_period') && ppa_debt==true )",
        "33": "\t\t\tmsg = msg + 'IRR target year is greater than the analysis period. Choose a target year less than the analysis period. ';",
        "34": "      }",
        "35": "    }        ",
        "36": "  }",
        "37": "  return msg;",
        "38": "}",
        "39": "",
        "40": "on_change{'analysis_period'} = define() {",
        "41": "  if  ( financing() == 'Merchant Plant') {",
        "42": "\tmsgbox('Analysis period changed!\\nThe merchant plant price data may not match the analysis period. Go to the Revenue page to update the Energy Market or Ancillary Service price data as appropriate.');",
        "43": "\tvalue('mp_energy_market_revenue', value('mp_energy_market_revenue_ui'));",
        "44": "\tvalue('mp_ancserv1_revenue', value('mp_ancserv1_revenue_ui'));",
        "45": "\tvalue('mp_ancserv2_revenue', value('mp_ancserv2_revenue_ui'));",
        "46": "\tvalue('mp_ancserv3_revenue', value('mp_ancserv3_revenue_ui'));",
        "47": "\tvalue('mp_ancserv4_revenue', value('mp_ancserv4_revenue_ui'));",
        "48": "\tvalue('mp_energy_market_revenue_single', value('mp_energy_market_revenue_single_ui'));",
        "49": "\tvalue('mp_ancserv1_revenue_single', value('mp_ancserv1_revenue_single_ui'));",
        "50": "\tvalue('mp_ancserv2_revenue_single', value('mp_ancserv2_revenue_single_ui'));",
        "51": "\tvalue('mp_ancserv3_revenue_single', value('mp_ancserv3_revenue_single_ui'));",
        "52": "\tvalue('mp_ancserv4_revenue_single', value('mp_ancserv4_revenue_single_ui'));",
        "53": "  }",
        "54": "  if (( technology() == 'Flat Plate PV') ||  ( technology() == 'PV Battery')){",
        "55": "  \tvalue('ac_lifetime_losses', value('ac_lifetime_losses_ui'));",
        "56": "  \tvalue('dc_lifetime_losses', value('dc_lifetime_losses_ui'));",
        "57": "  }",
        "58": "  txt = analysis_period_message();",
        "59": "  property('analysis_period_warning','caption',txt);",
        "60": "};",
        "61": ""
    }
}