    "Equations": {
        "1": "equations{ 'cp_system_nameplate' } = define() {",
        "2": "\tif (technology() == \"Standalone Battery\") return ${batt_power_discharge_max_kwac} / 1000;",
        "3": "\telse return ${system_capacity}/1000;",
        "4": "};",
        "5": "",
        "6": "equations{ 'ui_system_nameplate_standalone'} = define() {",
        "7": "\tif (technology() == \"Standalone Battery\") return ${cp_system_nameplate};",
        "8": "\telse return 0;",
        "9": "};",
        "10": "",
        "11": "equations{ 'cp_battery_nameplate' } = define() {",
        "12": "\tt = technology();",
        "13": "\tif ( (t=='PV Battery') || (t=='Generic Battery') || (t=='Standalone Battery') )",
        "14": "\t\t{ x = ${batt_power_discharge_max_kwac}/1000; }",
        "15": "\telseif ( t == 'PVWatts Battery' )\t",
        "16": "\t\t{ x = ${batt_simple_kw}/1000; }",
        "17": "\telseif ( t == 'ETES' )",
        "18": "\t\t{ x = ${cp_system_nameplate}; }",
        "19": "\telse",
        "20": "\t\t{ x = 0; }",
        "21": "\treturn x;\t",
        "22": "};",
        "23": "",
        "24": "//capacity credit % from ISO based on battery duration?",
        "25": "//4 hours of duration at ?? MW capacity to qualify for payment",
        "26": "//Sysstem nameplate in AC",
        "27": "//rename system nameplate to nominal bank power when tech = standalone"
    },
    "Callbacks": {
        "1": "function capacity_payments_ctrl_refresh()",
        "2": "{",
        "3": "\tfixed_payment = (value(\"cp_capacity_payment_type\") == 1);",
        "4": "\tshow(\"cp_payment_units\",!fixed_payment);",
        "5": "\tshow(\"cp_payment_units_dollars\",fixed_payment);",
        "6": "",
        "7": "\t//enable(\"cp_capacity_credit_group\",!fixed_payment);",
        "8": "\tenable(\"cp_capacity_credit_percent\",!fixed_payment);",
        "9": "",
        "10": "\tenable(\"cp_system_nameplate\",!fixed_payment);",
        "11": "\tenable(\"cp_battery_nameplate\",!fixed_payment);",
        "12": "\t",
        "13": "\tcp_payment = value(\"cp_capacity_payment_amount\");",
        "14": "\tshow_escalation = (#cp_payment < 2);",
        "15": "\tenable(\"cp_capacity_payment_esc\",show_escalation);",
        "16": "",
        "17": "\trefresh();",
        "18": "}",
        "19": "",
        "20": "on_load{\"Financial Capacity Payments\"} = define() {",
        "21": "\tcapacity_payments_ctrl_refresh();",
        "22": "\tcapacity_payment_options();",
        "23": "\tproperty('cp_capacity_payment_amount','FixedLength',value('analysis_period'));",
        "24": "};",
        "25": "",
        "26": "on_change{\"cp_capacity_payment_type\"} =",
        "27": "on_change{\"cp_capacity_payment_amount\"} = define()",
        "28": "{",
        "29": "\tcapacity_payments_ctrl_refresh();",
        "30": "};\t",
        "31": "",
        "32": "function capacity_payment_options()",
        "33": "{",
        "34": "\ttech = technology();",
        "35": "\tif (tech == \"Standalone Battery\") {",
        "36": "\t\tshow('cp_system_nameplate', false);",
        "37": "\t\tshow('cp_battery_nameplate', true);",
        "38": "\t\tshow('battery_nameplate_text', false);",
        "39": "\t\tproperty('capacity_payments_text', 'Caption', 'For projects that earn revenue from capacity payments, choose the capacity basis or fixed amount option. For Capacity Basis, the annual capacity payment is the product of the capacity payment amount ($/MW-yr), battery maximum discharge power (MWac), and eligible percent of nameplate (%). For Fixed Amount, the capacity payment is the amount you enter.');",
        "40": "\t}",
        "41": "\telse if (tech == \"PV Battery\" || tech == \"Generic Battery\" || tech == \"PVWatts Battery\") {",
        "42": "\t\tshow('cp_battery_nameplate', true);",
        "43": "\t\tshow('cp_system_nameplate', true);",
        "44": "\t\tproperty('capacity_payments_text', 'Caption', 'For projects that earn revenue from capacity payments, choose the capacity basis or fixed amount option. For Capacity Basis, the annual capacity payment is the product of the capacity payment amount ($/MW-yr), system nameplate capacity (MWdc for PV systems, MWac for other systems), and eligible percent of nameplate (%). For Fixed Amount, the capacity payment is the amount you enter.');",
        "45": "\t\tshow('battery_nameplate_text', true);",
        "46": "\t}",
        "47": "\telse {",
        "48": "\t\tshow('cp_system_nameplate', true);",
        "49": "\t\tshow('cp_battery_nameplate', false);",
        "50": "\t\tproperty('capacity_payments_text', 'Caption', 'For projects that earn revenue from capacity payments, choose the capacity basis or fixed amount option. For Capacity Basis, the annual capacity payment is the product of the capacity payment amount ($/MW-yr), system nameplate capacity (MWdc for PV systems, MWac for other systems), and eligible percent of nameplate (%). For Fixed Amount, the capacity payment is the amount you enter.');",
        "51": "\t\tshow('battery_nameplate_text', false);",
        "52": "\t}",
        "53": "\trefresh();",
        "54": "}",
