    "Equations": {
        "1": "equations{'forecast_price_signal_model'} = define()",
        "2": "{",
        "3": "\tif (financing() == 'Merchant Plant')",
        "4": "\t\treturn 1;",
        "5": "\telse",
        "6": "\t\treturn 0;",
        "7": "};",
        "8": "/*",
        "9": "equations{ '$MIMO$ mp_analysis_period_changed' } = define() {",
        "10": "\t${mp_energy_market_revenue} = ${mp_energy_market_revenue_ui};",
        "11": "\treturn 0;",
        "12": "};",
        "13": "*/",
        "14": "",
        "15": "equations{ '$MIMO$ mp_check_cleared_capacity' } = define() {",
        "16": "\tdata.analysis_period =  ${analysis_period};",
        "17": "\tdata.system_capacity =  ${system_capacity};",
        "18": "\tdata.mp_enable_energy_market_revenue =  ${mp_enable_energy_market_revenue};",
        "19": "\tdata.mp_enable_ancserv1 =  ${mp_enable_ancserv1};",
        "20": "\tdata.mp_enable_ancserv2 =  ${mp_enable_ancserv2};",
        "21": "\tdata.mp_enable_ancserv3 =  ${mp_enable_ancserv3};",
        "22": "\tdata.mp_enable_ancserv4 =  ${mp_enable_ancserv4};",
        "23": "\tdata.mp_energy_market_revenue =  ${mp_energy_market_revenue};",
        "24": "\tdata.mp_ancserv1_revenue =  ${mp_ancserv1_revenue};",
        "25": "\tdata.mp_ancserv2_revenue =  ${mp_ancserv2_revenue};",
        "26": "\tdata.mp_ancserv3_revenue =  ${mp_ancserv3_revenue};",
        "27": "\tdata.mp_ancserv4_revenue =  ${mp_ancserv4_revenue};",
        "28": "\tdata.mp_energy_market_revenue_single =  ${mp_energy_market_revenue};",
        "29": "\tdata.mp_ancserv1_revenue_single =  ${mp_ancserv1_revenue_single};",
        "30": "\tdata.mp_ancserv2_revenue_single =  ${mp_ancserv2_revenue_single};",
        "31": "\tdata.mp_ancserv3_revenue_single =  ${mp_ancserv3_revenue_single};",
        "32": "\tdata.mp_ancserv4_revenue_single =  ${mp_ancserv4_revenue_single};",
        "33": "",
        "34": "\tsuccess = ssc_eqn('mp_ancillary_services', data);",
        "35": "\t",
        "36": "\tif (!success){",
        "37": "\t\tmsgbox('success ' + success);",
        "38": "\t\treturn 0;",
        "39": "\t}",
        "40": "\t",
        "41": "\tif (data.mp_capacity_check == 0)",
        "42": "\t{",
        "43": "\t\tmsg = data.mp_capacity_check_error;",
        "44": "\t\t${mp_error_msg} = msg;",
        "45": "\t\tmsgbox(msg);",
        "46": "\t}",
        "47": "\treturn 0;",
        "48": "};",
        "49": ""
    },
    "Callbacks": {
        "1": "function update_merchant_plant_inputs()",
        "2": "{",
        "3": "\tmarket_enable = value('mp_enable_energy_market_revenue');",
        "4": "\tenable('mp_energy_market_revenue', market_enable && !value('mp_enable_market_percent_gen'));",
        "5": "\tenable('mp_energy_market_revenue_single', market_enable && value('mp_enable_market_percent_gen'));",
        "6": "\tenable('mp_enable_market_percent_gen', market_enable);",
        "7": "\tenable('mp_market_percent_gen',market_enable && value('mp_enable_market_percent_gen'));",
        "8": "}",
        "9": "",
        "10": "on_load{'Financial Merchant Plant Energy Market Revenue'} = define() {",
        "11": "\tupdate_merchant_plant_inputs();",
        "12": "};",
        "13": "",
        "14": "on_change{'mp_enable_energy_market_revenue'} = define() {",
        "15": "\tupdate_merchant_plant_inputs();",
        "16": "};",
        "17": "",
        "18": "on_change{'mp_enable_market_percent_gen'} = define() {",
        "19": "\tupdate_merchant_plant_inputs();",
