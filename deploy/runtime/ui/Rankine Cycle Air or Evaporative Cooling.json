        "19": "\telseif(${csp.pt.pwrb.pressure_mode}==2) //first custom cycle",
        "20": "\t\treturn 5;",
        "21": "\telseif(${csp.pt.pwrb.pressure_mode}==3) //second custom cycle",
        "22": "\t\treturn 6;",
        "23": "\telse // fixed pressure",
        "24": "\t\treturn 1; // tower",
        "25": "};",
