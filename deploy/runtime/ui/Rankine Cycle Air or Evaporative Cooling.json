        "50": "",
        "51": "function update_acc_press_plot()",
        "52": "{",
        "53": "\ttable = acc_press_vs_load_temp();",
        "54": "\tdesPcond = table{'desPcond'};",
        "55": "\tp_cond = table{'p_cond'};",
        "56": "\tTamb = table{'Tamb'};",
        "57": "\t",
        "58": "\t",
        "59": "\tclearplot( 'acc_pressure_plot');",
        "60": "\tplot( Tamb, p_cond, {'thick'=3, 'type' = 'line', 'color' = 'blue'});//, 'series' = 'Tamb: ' + ${T_amb_des} + ' [C]'",
        "61": "\tplotopt( {'title' = 'Air-cooled Condenser Model (full-load)', 'legend' = true});//, 'legendpos'=[15,12]",
        "62": "\taxis( 'x1', {'label' = 'Ambient Temperature [C]', 'min' = 0, 'max' = ${T_amb_des}*1.1});",
        "63": "\taxis( 'y1', {'label' = 'Condenser Pressure [inHg]', 'min'= 0, 'max' = desPcond*1.1});",
        "64": "}",
        "65": "",
        "66": "function acc_press_vs_load_temp()",
        "67": "{",
        "68": "\tP_cond_min = ${P_cond_min};",
        "69": "\tT_amb_des = ${T_amb_des};",
        "70": "\t",
        "71": "\t// Fitted regression model coefficients",
        "72": "\tA = [[147.96619, 71.23482281, 27.55395547],",
        "73": "\t\t [-329.021562, -159.2675368, -62.24857193],",
        "74": "\t\t [183.4601872, 89.50235831, 35.57127305]];",
        "75": "\t",
        "76": "\tNsteps = 10;",
        "77": "\t",
        "78": "\t// Normalized factors ",
        "79": "\tp_norm = 0.;",
        "80": "\tT_norm = 1.0;\t// normalized ambient temperature",
        "81": "\tq_norm = 1.; \t// Approximately Full load",
        "82": "\tt_step = (T_norm - 0.85)/Nsteps;",
        "83": "\tfor (t = 0; t <= Nsteps + 1; t++){",
        "84": "\t\tif (T_norm > 0.8925){",
        "85": "\t\t\tfor (i = 0; i < #A; i++){",
        "86": "\t\t\t\tfor (j = 0; j < #A[i]; j++){",
        "87": "\t\t\t\t\tp_norm += A[i][j]*pow(T_norm, i)*pow(q_norm,j);",
        "88": "\t\t\t\t}",
        "89": "\t\t\t}",
        "90": "\t\t}",
        "91": "\t\telse",
        "92": "\t\t\tp_norm = 1;",
        "93": "\t",
        "94": "\t\tT_vec[t] = T_norm*(T_amb_des+273.15) - 273.15;",
        "95": "\t\tif (T_norm == 1.0)",
        "96": "\t\t\tdesPcond = p_norm*P_cond_min;",
        "97": "\t\t\t",
        "98": "\t\tif (p_norm > 1)",
        "99": "\t\t\tp_vec[t] = p_norm*P_cond_min;",
        "100": "\t\telse",
        "101": "\t\t\tp_vec[t] = P_cond_min;",
        "102": "\t\t// step down",
        "103": "\t\tT_norm -= t_step;",
        "104": "\t\tp_norm = 0.0; // Reset p",
        "105": "\t}",
        "106": "\t",
        "107": "\ttable{'desPcond'} = desPcond;",
        "108": "\ttable{'Tamb'} = T_vec;",
        "109": "\ttable{'p_cond'} = p_vec;",
        "110": "\treturn table;",
        "111": "}",
        "112": "",
        "113": "",
        "114": "",
        "115": ""
    }
