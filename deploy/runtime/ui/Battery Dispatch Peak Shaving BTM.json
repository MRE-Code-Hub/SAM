        "37": "",
        "38": "function toggle_forecast_standalone_peak() ",
        "39": "{",
        "40": "\ttech = (technology() == 'Standalone Battery');",
        "41": "\tenable('peak_shaving_batt_dispatch_choice', tech == 0);",
        "42": "}",
        "43": "",
        "44": "function peak_shaving_choice() ",
        "45": "{",
        "46": "\tvalue('batt_dispatch_choice', 0);",
        "47": "\tchoice = value('peak_shaving_batt_dispatch_choice');",
        "48": "\ttech = (technology() == 'Standalone Battery');",
        "49": "\t// Turn on/off forecast input",
        "50": "\t//toggle_forecast();",
        "51": "\tenable('batt_select_weather_file_forecast_peak_shaving', choice == 2 && tech == 0);",
        "52": "\tenable('batt_user_specified_weather_file_peak_shaving', choice == 2 && tech == 0);",
        "53": "\tenable('batt_pv_ac_forecast_peak', choice == 2 && tech == 0);",
        "54": "",
        "55": "\tvalue('batt_dispatch_wf_forecast_choice', choice);",
        "56": "\tif (choice == 2)",
        "57": "\t{",
        "58": "\t\tac_forecast = value(\"batt_pv_ac_forecast\");",
        "59": "\t\tclipping_forecast = value(\"batt_pv_clipping_forecast\");",
        "60": "\t\tforecast_array = value(\"batt_pv_ac_forecast_peak\");",
        "61": "\t\tif (#ac_forecast <= 1 || #clipping_forecast <= 1) { ",
        "62": "\t\t\tif (technology() == 'PV Battery') {",
        "63": "\t\t\t\tfile = ${batt_user_specified_weather_file_peak_shaving};",
        "64": "\t\t\t\tif (file != \"\") {",
        "65": "\t\t\t\t\trun_forecast(file);",
        "66": "\t\t\t\t}",
        "67": "\t\t\t}",
        "68": "\t\t\telse {",
        "69": "\t\t\t\t// Generic system",
        "70": "\t\t\t    if (#forecast_array > 1) {",
        "71": "\t\t\t\t\trun_generic_forecast(forecast_array);",
        "72": "\t\t\t    }",
        "73": "\t\t\t}",
        "74": "\t\t}",
        "75": "\t\t\t",
        "76": "\t}",
        "77": "\telse {",
        "78": "\t\t${batt_pv_ac_forecast} = [0];",
        "79": "\t\t${batt_pv_clipping_forecast} = [0];",
        "80": "\t}",
        "81": "\t",
        "82": "\tload_choice = value(\"batt_dispatch_load_forecast_choice_peak_shaving\");",
        "83": "\tenable('load_user_forecast_data_peak_shaving', load_choice == 2);",
        "84": "\tenable('batt_dispatch_load_escal_choice_peak', load_choice == 2);",
