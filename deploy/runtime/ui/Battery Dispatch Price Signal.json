        "102": "\tenable('load_user_forecast_data_psd', load_choice == 2);",
        "103": "\tenable('batt_dispatch_load_escal_choice_psd', load_choice == 2);",
        "104": "\tenable('batt_dispatch_load_forecast_escal_psd', load_choice == 2 && ${batt_dispatch_load_escal_choice_psd} == 1);",
