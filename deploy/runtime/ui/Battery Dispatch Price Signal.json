        "75": "\t{",
        "76": "\t\tac_forecast = value(\"batt_pv_ac_forecast\");",
        "77": "\t\tclipping_forecast = value(\"batt_pv_clipping_forecast\");",
        "78": "\t\tforecast_array = value(\"batt_pv_ac_forecast_psd\");",
