    "Equations": {
        "1": "equations{'batt_simple_meter_position'} = define()",
        "2": "{",
        "3": "\t// behind the meter default",
        "4": "\tbatt_meter_position = 0;",
        "5": "\tfinancial_model = financing();",
        "6": "\t",
        "7": "\tif (!strcmp(financial_model, 'Single Owner'))",
        "8": "\t\tbatt_meter_position = 1;",
        "9": "",
        "10": "\treturn batt_meter_position;",
        "11": "};",
        "12": ""
    },
    "Callbacks": {
        "1": "on_load{'Battery Model Simple with REopt'} = define(){",
        "2": "\tenable(\"batt_custom_dispatch\", (value(\"batt_simple_dispatch\") == 2));",
        "3": "};",
        "4": "",
        "5": "on_change{\"batt_simple_dispatch\"} = define(){",
        "6": "\tenable(\"batt_custom_dispatch\", (value(\"batt_simple_dispatch\") == 2));",
        "7": "};",
        "8": "",
        "9": "on_change{'call_reopt'} = define(){",
        "10": "\tdispatch_original_mode = value(\"batt_simple_dispatch\"); //save original dispatch mode",
        "11": "\tbatt_kw_original = value(\"batt_simple_kw\");",
        "12": "\tbatt_kwh_original = value(\"batt_simple_kwh\");",
        "13": "",
        "14": "\twf_len = 60 / ${ui_step_minutes} * 8760;",
        "15": "\tload_len = #(${load});",
        "16": "\tif (wf_len != load_len) {",
        "17": "\t\tmsgbox(\"Weather file and load number of records must be the same.\");",
        "18": "\t\texit;",
        "19": "\t}",
        "20": "\tif (load_len != 8760 * 4 && load_len != 8760 * 2 && load_len != 8760) {",
        "21": "\t\tmsgbox(\"Simulation timestep must be 15, 30 or 60 minutes.\");",
        "22": "\t\texit;",
        "23": "\t}",
        "24": "\t",
        "25": "\t// reset size to 0 to remove battery costs from total_installed_cost",
        "26": "\tvalue(\"batt_simple_kw\", 0);",
        "27": "\tvalue(\"batt_simple_kwh\", 0); ",
        "28": "\tvalue(\"batt_simple_dispatch\",0); //set dispatch mode to 0 to run ReOPT",
        "29": "\t",
        "30": "\treopt = reopt_size_battery();",
        "31": "\ty = reopt.response;",
        "32": "\tif (reopt.error){",
        "33": "\t\tif (strlen(reopt.error) > 0){",
        "34": "\t\t\tmsgbox(reopt.error);",
        "35": "\t\t}",
        "36": "\t\telse{",
        "37": "\t\t\tmsgbox(\"REopt API timed out. Please try again later.\");",
        "38": "\t\t}",
        "39": "\t\tvalue(\"batt_simple_dispatch\",dispatch_original_mode); //restore original dispatch mode for early exit from ReOPT",
        "40": "\t\tvalue(\"batt_simple_kw\", batt_kw_original);",
        "41": "\t\tvalue(\"batt_simple_kwh\", batt_kwh_original); ",
        "42": "\t\texit;",
        "43": "\t}",
        "44": "",
        "45": "\tif (y.outputs == null || y.outputs.Scenario == null){",
        "46": "\t\tmsgbox(\"Error in REopt response. Try again later or try another system design.\");",
        "47": "\t\tvalue(\"batt_simple_dispatch\",dispatch_original_mode); //restore original dispatch mode for early exit from ReOPT",
        "48": "\t\tvalue(\"batt_simple_kw\", batt_kw_original);",
        "49": "\t\tvalue(\"batt_simple_kwh\", batt_kwh_original); ",
        "50": "\t\texit;",
        "51": "\t}",
        "52": "\t",
        "53": "\tpv = y.outputs.Scenario.Site.PV;",
        "54": "\tbatt = y.outputs.Scenario.Site.Storage;",
        "55": "\telec = y.outputs.Scenario.Site.ElectricTariff;",
        "56": "",
        "57": "\tif (batt.size_kw == null && batt.size_kwh == null){",
        "58": "\t\tapply_str = \"Apply results?\\nOptimal capacity (kWh): \" + to_string(0) + ",
        "59": "\t\t\t\"\\nOptimal power (kW): \" + to_string(0) + \"\\n\\nReplace \" + ",
        "60": "\t\t\t\"'Battery capacity' and 'Battery power' with results from REopt?\";",
        "61": "\t\tapply = yesno(apply_str);",
        "62": "\t\tif (apply){",
        "63": "\t\t\tvalue('batt_simple_kw', 0.0);",
        "64": "\t\t\tvalue('batt_simple_kwh', 0.0);",
        "65": "\t\t}",
        "66": "\t\tvalue(\"batt_simple_dispatch\",dispatch_original_mode); //restore original dispatch mode for early exit from ReOPT",
        "67": "\t\tvalue(\"batt_simple_kw\", batt_kw_original);",
        "68": "\t\tvalue(\"batt_simple_kwh\", batt_kwh_original); ",
        "69": "\t\texit;",
        "70": "\t}",
        "71": "",
        "72": "\tapply_str = \"Apply results?\\nOptimal capacity (kWh): \" + to_string(batt.size_kwh) + ",
        "73": "\t\t\"\\nOptimal power (kW): \" + to_string(batt.size_kw) + \"\\n\\nReplace \" +",
        "74": "\t\t\"'Battery capacity', 'Battery power', and 'Custom dispatch' with results from REopt?\";",
        "75": "\tapply = yesno(apply_str);",
        "76": "\tif (!apply) {",
        "77": "\t\tvalue(\"batt_simple_dispatch\",dispatch_original_mode); //restore original dispatch mode for early exit from ReOPT",
        "78": "\t\tvalue(\"batt_simple_kw\", batt_kw_original);",
        "79": "\t\tvalue(\"batt_simple_kwh\", batt_kwh_original); ",
        "80": "\t\texit;",
        "81": "\t}",
        "82": "\t",
        "83": "\tvalue('batt_simple_kw', batt.size_kw);",
