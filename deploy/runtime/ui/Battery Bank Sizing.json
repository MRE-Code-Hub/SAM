        "35": "};",
        "36": "on_change{'batt_bank_duration'} = define()",
        "37": "{",
        "38": "\t",
        "39": "\tis_cap = (value('batt_duration_choice') == 0);",
        "40": "\tif (!is_cap) {",
        "41": "\t\tbank_power = value('batt_bank_power');",
        "42": "\t\tbank_duration = value('batt_bank_duration');",
        "43": "\t\tvalue('batt_bank_size_ui',bank_power * bank_duration);",
        "44": "\t}",
        "45": "\tbattsize_warning_check();",
        "46": "};",
        "47": "function set_bank_sizing()",
        "48": "{",
        "49": "\tisauto = (value('batt_size_choice') == 0);",
        "50": "\tisflow = is_flow();",
        "51": "",
        "52": "\t// auto-sizing",
        "53": "\tenable('batt_bank_size', isauto);",
        "54": "\tenable('batt_bank_size_ui', isauto);",
        "55": "\tenable('batt_bank_power', isauto);",
        "56": "\tenable('batt_bank_voltage', isauto);",
        "57": "\tenable('batt_bank_size_dc_ac', isauto);",
        "58": "\tenable('batt_duration_choice', isauto);",
        "59": "\tenable('batt_bank_duration', isauto);",
        "60": "\t// manual specify",
        "61": "\tshow('batt_bank_size_specify', isflow);",
        "62": "\tshow('batt_C_rate_max_charge_input', !isflow);",
        "63": "\tshow('batt_C_rate_max_discharge_input', !isflow);",
        "64": "",
        "65": "\tenable('batt_bank_ncells_serial', !isauto);",
        "66": "\tenable('batt_bank_nstrings', !isauto);",
        "67": "\tenable('batt_bank_nseries_stacks', !isauto);",
        "68": "\t",
        "69": "",
        "70": "\tif (isflow)",
        "71": "\t\tenable('batt_bank_size_specify', !isauto);",
        "72": "\telse",
        "73": "\t{",
        "74": "\t\tenable('batt_C_rate_max_charge_input', !isauto);",
        "75": "\t\tenable('batt_C_rate_max_discharge_input', !isauto);",
        "76": "\t}",
        "77": "}",
        "78": "",
        "79": "function set_duration_choice()",
        "80": "{",
        "81": "\tis_cap = (value('batt_duration_choice') == 0);",
        "82": "\t",
        "83": "\tshow('batt_bank_duration',!is_cap);",
        "84": "\tshow('batt_bank_size_ui',is_cap);",
        "85": "\tbank_power = value('batt_bank_power');",
        "86": "\tbank_cap = value('batt_bank_size_ui');",
        "87": "\tif (!is_cap)",
        "88": "\t{",
        "89": "\t\tduration = bank_cap / bank_power;",
        "90": "\t\tvalue('batt_bank_duration', duration);",
        "91": "\t}",
        "92": "\telse",
        "93": "\t{",
        "94": "\t\tduration = value('batt_bank_duration');",
        "95": "\t\tbank_cap_calc = duration * bank_power;",
        "96": "\t\tvalue('batt_bank_size_ui', bank_cap_calc);",
        "97": "\t}",
        "98": "\t",
        "99": "\trefresh();",
        "100": "}",
        "101": "",
        "102": "function calc_batt_size() {",
        "103": "\tis_cap = (value('batt_duration_choice') == 0);",
        "104": "\tif (is_cap) {",
        "105": "\t\tbank_cap = value('batt_bank_size');",
        "106": "\t\tbank_power = value('batt_bank_power');",
        "107": "\t\tbank_duration = bank_cap / bank_power;",
        "108": "\t\tvalue('batt_bank_duration',batt_duration);",
        "109": "\t}",
        "110": "\telse {",
        "111": "\t\tbank_duration = value('batt_bank_duration');",
        "112": "\t\tbank_power = value('batt_bank_power');",
        "113": "\t\tbank_cap = bank_power * bank_duration;",
        "114": "\t\tvalue('batt_bank_size', bank_cap);",
        "115": "\t}",
        "116": "}",
        "117": "\t"
    }
