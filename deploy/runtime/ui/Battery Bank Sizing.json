    },
    "Callbacks": {
        "1": "on_load{'Battery Bank Sizing'} = define()",
        "2": "{",
        "3": "\tset_chemistry();",
        "4": "\tset_bank_sizing();",
        "5": "\t//set_duration_choice();",
        "6": "\tis_cap = (value('batt_duration_choice') == 0);",
        "7": "\t",
        "8": "\tshow('batt_bank_duration',!is_cap);",
        "9": "\tshow('batt_bank_size_ui',is_cap);",
        "10": "\trefresh();",
        "11": "\t",
        "12": "\ton_change{'batt_size_choice'};",
        "13": "\ton_change{'batt_bank_size'};",
        "14": "\ton_change{'batt_bank_power'};",
        "15": "\ton_change{'bank_bank_size_specify'};",
        "16": "\ton_change{'batt_duration_choice'};",
        "17": "\ton_change{'batt_bank_duration'};",
        "18": "};",
        "19": "on_change{'batt_size_choice'} = define()",
        "20": "{",
        "21": "\tset_bank_sizing();",
        "22": "\tbattsize_warning_check();",
