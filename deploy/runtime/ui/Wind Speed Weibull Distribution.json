    "Callbacks": {
        "1": "function plot_weibull()",
        "2": "{",
        "3": "\tnp = 161; ",
        "4": "\tweibullk = value('weibull_k_factor');\t",
        "5": "\tws = alloc(np);",
        "6": "\tweibull_probability = alloc(np);",
        "7": "\t",
        "8": "\tws[0] = 0;",
        "9": "\tweibull_probability[0] = 0;",
        "10": "\t",
        "11": "\thub_ht_windspeed = pow((value(\"wind_turbine_hub_ht\")/value('weibull_reference_height')),value(\"wind_resource_shear\")) * value('weibull_wind_speed');",
        "12": "\tdenom = exp(gammaln(1+(1/weibullk)));",
        "13": "\tlambda = hub_ht_windspeed/denom;",
        "14": "\t",
        "15": "\tfor (i=1;i<np;i++)",
        "16": "\t{",
        "17": "\t\tws[i] = i/4;",
        "18": "\t\tweibull_probability[i] = ( (weibullk / pow(lambda,weibullk)) * pow(ws[i],(weibullk - 1)) * exp(-pow(ws[i]/lambda,weibullk)) );",
        "19": "\t}",
        "20": "\t",
        "21": "\tsetplot(\"wind_characteristics_plot\");",
        "22": "\tclearplot('wind_characteristics_plot');",
        "23": "\tplot( ws, weibull_probability, {\"thick\"=1, \"scale\"=true, \"color\"=[100,200,100], \"series\"=\"Weibull Probability\"});",
        "24": "",
        "25": "\taxis( \"x1\", {\"min\"=0,  \"label\"=\"Wind Speed\"});",
        "26": "\taxis( \"y1\", {\"min\"=0,  \"label\"=\"Likelihood\"});",
        "27": "\taxis( \"y2\", {\"min\"=0,  \"label\"=\"Energy\"});\t",
        "28": "\t",
        "29": "\tif(value(\"wind.turbine.radio_list_or_design\") == 1)",
        "30": "\t{",
        "31": "\t\tturbine_pc = ${wind_turbine_powercurve_powerout};",
        "32": "\t\tturbine_energy = alloc(#ws);",
        "33": "\t\tturbine_energy[0] = 0;",
        "34": "",
        "35": "\t\tfor (i=1;i<np;i++)",
        "36": "\t\t\tturbine_energy[i] = turbine_pc[i] * weibull_probability[i];",
        "37": "\t\t",
        "38": "\t\tplot( ws, turbine_energy,  {\"thick\"=1, \"scale\"=true, \"color\"=[50,50,250],    \"series\"=\"Turbine Energy\",  \"yap\"= \"right\"});",
        "39": "\t}",
        "40": "",
        "41": "}",
        "42": "\t",
        "43": "",
        "44": "on_load{\"Wind Speed Weibull Distribution\"}=define(){",
        "45": "\t//enable(\"wind_characteristics_elevation\", (${wind.turbine.radio_list_or_design} == 1) );",
        "46": "",
        "47": "\tsetplot(\"wind_characteristics_plot\");",
        "48": "\tplotopt( {\"title\"=\"\",\"backcolor\"=[255,255,255], \"popup\"=true, \"legend\"=true, \"legendpos\"=[75,10]}  );",
        "49": "\tplot_weibull();\t",
        "50": "};\t",
        "51": "",
        "52": "on_change{'weibull_wind_speed'} = define() { plot_weibull(); };",
        "53": "on_change{'weibull_k_factor'} = define() { plot_weibull(); };",
        "54": "on_change{'weibull_reference_height'} = define() {plot_weibull(); };",
        "55": "//on_change{\"wind_characteristics_elevation\"} = define() { plot_weibull(); };",
