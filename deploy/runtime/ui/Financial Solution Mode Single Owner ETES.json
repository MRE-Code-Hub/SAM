    "Callbacks": {
        "1": "// This form is a copy of Financial Solution Flip Leaseback",
        "2": "// so ppa_soln_mode radio button can have different labels",
        "3": "// to indicated IRR target is for the tax investor for flip ",
        "4": "// structures. Any updates should be made to both forms.",
        "5": "",
        "6": "on_load{'Financial Solution Mode Single Owner ETES'} = define()",
        "7": "{",
        "8": "\tsoln_mode_ctrl_refresh();",
        "9": "\tdisplay_message();",
        "10": "};",
        "11": "",
        "12": "on_change{'ppa_soln_mode'} = define()",
        "13": "{",
        "14": "\tsoln_mode_ctrl_refresh();",
        "15": "\tdisplay_message();",
        "16": "};",
        "17": "",
        "18": "on_change{'ppa_price_input'} = define()",
        "19": "{",
        "20": "\tsoln_mode_ctrl_refresh();",
        "21": "};",
        "22": "",
        "23": "on_change{'flip_target_year'} = define () {",
        "24": "\tdisplay_message();",
        "25": "};",
        "26": "",
        "27": "function display_message() ",
        "28": "{",
        "29": "\ttxt = '';",
        "30": "\tclr = 'black';",
        "31": "\tif ( value('flip_target_year') > value('analysis_period') )",
        "32": "\t{",
        "33": "\t\ttxt += 'IRR target year is greater than the analysis period. ';",
        "34": "\t\tclr = 'red';",
        "35": "\t}",
        "36": "\tif ( financing() == 'Host Developer' ) ",
        "37": "\t{",
        "38": "\t\ttxt += 'IRR is the developer internal rate of return. ';",
        "39": "\t}",
        "40": "\telseif ( value('ppa_soln_mode') == 0 && value('en_electricity_rates') == 0 )",
        "41": "\t{",
        "42": "\t\tclr = 'red';",
        "43": "\t\ttxt += 'The Specify IRR Target option is not available with the Use PPA Price option on Electricity Purchases page. Either change the option on the Electricity Purchases page or use the Specify PPA Price option.';",
        "44": "\t}",
        "45": "\tproperty('solution_mode_message','Caption',txt);",
        "46": "\tproperty('solution_mode_message','TextColour',clr);",
        "47": "}",
        "48": "",
        "49": "function soln_mode_ctrl_refresh()",
        "50": "{",
        "51": "    is_specify_irr = false;",
        "52": "\tshow_escalation = (#${ppa_price_input} < 2);",
        "53": "\tenable(\"ppa_escalation\",(show_escalation || is_specify_irr));",
        "54": "}"
    }
