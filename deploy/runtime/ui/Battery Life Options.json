    "Callbacks": {
        "1": "on_load{'Battery Life Options'} = define() {",
        "2": "\tbattery_type_message();",
        "3": "\tlife_message();",
        "4": "\t// batt_life_model is set on battery life forms",
        "5": "\t// for exclusive tabs for batt techs except fuel cell",
        "6": "\tif ( technology()=='Fuel Cell' )",
        "7": "\t{",
        "8": "\t\tvalue('batt_life_model',${ui_batt_life_model});",
        "9": "\t}",
        "10": "};",
        "11": "",
        "12": "on_change{'ui_batt_life_model'} = define(){",
        "13": "\tbattery_type_message();",
        "14": "\tlife_message();",
        "15": "\tif ( technology() != 'Fuel Cell' )",
        "16": "\t{",
        "17": "\t\tset_batt_life_excl_var();",
        "18": "\t}\t",
        "19": "\telse // fuel cell only",
        "20": "\t{",
        "21": "\t\tvalue('batt_life_model',${ui_batt_life_model});",
        "22": "\t\tcycle_calendar_message();",
        "23": "\t}",
        "24": "};",
        "25": "",
        "26": "function set_batt_life_excl_var()",
        "27": "{",
        "28": "\tvalue('batt_life_excl', value('ui_batt_life_model'));",
        "29": "}",
        "30": "",
        "31": "function life_message()",
        "32": "{",
        "33": "\tmsg = '';",
        "34": "\tclr = 'Black';",
        "35": "\toptions = ['Tables','Li-ion NMC/Graphite','Li-ion LMO/LTO'];",
        "36": "\tif ( ${batt_chem} == 0 )",
        "37": "\t{",
        "38": "\t\tmsg = 'Only the Cycle and Calendar Degradation option is available for lead-acid and flow battery types.';",
        "39": "\t}",
        "40": "\telseif ( ${ui_batt_life_model} == 0 )",
        "41": "\t{",
        "42": "\t\tmsg = 'The Cycle and Calendar Degradation inputs below define battery degradation curves.';",
        "43": "\t}",
        "44": "\telse",
        "45": "\t{",
        "46": "\t\tmsg = 'This option uses a special battery life model for the ' + options[${ui_batt_life_model}] + ' battery type that combines cycle and calendar degradation.';",
        "47": "\t}",
        "48": "\tproperty('lbl_batt_life_message','Caption',msg);",
        "49": "\tproperty('lbl_batt_life_message','TextColour',clr);",
        "50": "}",
        "51": "",
        "52": "function battery_type_message()",
        "53": "{",
        "54": "\tclr = 'Black';",
        "55": "\tmsg = '';",
        "56": "\ttypes = ['Lead Acid: Flooded','Lead Acid: VRLA Gel','Lead Acid: VRLA AGM','Lead Acid: Custom','Lithium Ion: Lithium Manganese Oxide (LMO/Graphite)','Lithium Ion: Lithium Iron Phosphate (LFP/Graphite)','Lithium Ion: Lithium Cobalt Oxide (LCO/Graphite)','Lithium Ion: LMO/Lithium Titanate (LMO/LTO)','Lithium Ion: Nickel Manganese Cobalt Oxide (NMC/Graphite)','Lithium Ion: Nickel Cobalt Aluminum Oxide (NCA/Graphite)','Lithium Ion: Custom','Flow Battery: Vanadium','Flow Battery: Iron'];",
        "57": "\tif ( ${batt_chem} == 1 ) // Li-ion",
        "58": "\t{",
        "59": "\t\tif ( ${batt_type} == 7 ) // LMO/LTO",
        "60": "\t\t{",
        "61": "\t\t\tbest_life = 'Li-ion LMO/LTO';",
        "62": "\t\t\tif ( ${ui_batt_life_model} != 2 ) { clr = 'Red'; }",
        "63": "\t\t}",
        "64": "\t\telseif ( ${batt_type} == 8 ) // NMC/Graphite",
        "65": "\t\t{",
        "66": "\t\t\tbest_life = 'Li-ion NMC/Graphite'; ",
        "67": "\t\t\tif ( ${ui_batt_life_model} != 1 ) { clr = 'Red'; }",
        "68": "\t\t}",
        "69": "\t\telse // other Li-ion types",
        "70": "\t\t{",
        "71": "\t\t\tbest_life = 'empirical cycle and calendar degradation'; ",
        "72": "\t\t\tif ( ${ui_batt_life_model} != 0 || ${batt_calendar_choice} != 1 ) { clr = 'Red'; }",
        "73": "\t\t}",
        "74": "\t}",
        "75": "\telse // lead acid and flow",
        "76": "\t{",
        "77": "\t\tbest_life = 'cycle degradation';",
        "78": "\t\tif ( ${ui_batt_life_model} != 0 || ${batt_calendar_choice} != 0 ) { clr = 'Red'; }",
        "79": "\t}",
        "80": "\tmsg += 'The best battery life option for ' + types[${batt_type}] + ' (batt_type='+ ${batt_type}+')' + ' is ' + best_life + '.';",
        "81": "\tif ( clr == 'Red' ) { msg+=' Choose the appropriate battery life options here or reselect the battery type on the Battery Cell and System page to reset to the default options.'; }",
        "82": "\tproperty('lbl_batt_chem','Caption',msg);",
        "83": "\tproperty('lbl_batt_chem','TextColour',clr);",
        "84": "}",
        "85": ""
    }
