    "Equations": {
        "1": "// om_land_lease in $/acre",
        "2": "equations{ 'om_land_lease'} = define(){",
        "3": "\tif ( ${ui_land_area_units} == 1 )",
        "4": "\t{",
        "5": "\t\tfor ( i=0; i<#${ui_land_lease}; i++)",
        "6": "\t\t{",
        "7": "\t\t\tx[i] = ${ui_land_lease}[i] / 2.47105;",
        "8": "\t\t}",
        "9": "\t}",
        "10": "\telse",
        "11": "\t{",
        "12": "\t\tx = ${ui_land_lease};",
        "13": "\t}",
        "14": "\treturn x;",
        "15": "};",
        "16": "",
        "17": "// total_land_area must be defined on System Design form",
        "18": "// or equivalent for each technology",
        "19": "equations{ 'land_area' } = define() {",
        "20": "\ttech = technology();",
        "21": "\tif ( tech == 'Empirical Trough' ) { land_area = ${ui_total_land_area}; }",
        "22": "\telseif ( tech == 'MSPT') { land_area = ${csp.pt.sf.total_land_area}; }",
        "23": "\telseif ( tech == 'Generic CSP System') { land_area = ${csp.gss.solf.total_land_area}; }",
        "24": "\telseif ( tech == 'MSLF') { land_area = ${csp.mslf.cost.total_land_area}; }",
        "25": "\telseif ( tech == 'DSLF') { land_area = ${csp.lf.cost.total_land_area}; }\t",
        "26": "\telseif ( tech == 'High-X Concentrating PV') { land_area = ${hcpv.array.total_land_area}; }",
        "27": "\telseif ( tech == 'Flat Plate PV' || tech == 'PVWatts' || tech == 'Physical Trough' || tech == 'PV Battery' || tech == 'Fuel Cell')  { land_area = ${total_land_area}; }",
        "28": "\telse { land_area = 0; }",
        "29": "\treturn land_area;",
        "30": "};",
        "31": "",
        "32": "equations{ 'ui_land_area_ha' } = define()",
        "33": "{",
        "34": "\treturn ${land_area} / 2.47105;",
        "35": "};"
    },
    "Callbacks": {
        "1": "on_load{'Operating Costs Land Lease'} = define() {",
