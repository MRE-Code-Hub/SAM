        "12": "",
        "13": "function enable_lease_escalation() ",
        "14": "{",
        "15": "\tenable( 'om_land_lease_escal',#${ui_land_lease}==1);",
        "16": "\tunits=['$/acre','$/ha'];",
        "17": "\tproperty('ui_land_lease','Label',units[${ui_land_area_units}]);",
        "18": "\tproperty( 'ui_land_lease','FixedLength',${analysis_period});",
        "19": "}",
        "20": ""
