        "150": "function interpolate_dc_capex_table()",
        "151": "{",
        "152": "\tinput = ${capex_table_dc_inputs};",
        "153": "\t",
        "154": "\tsys_cap = ${modulearray_power_w};",
        "155": "\t",
        "156": "\tnr = #input;",
        "157": "\tnc = #input[0];",
        "158": "\t//refresh();",
        "159": "\tidx=0;",
        "160": "\t//Cap_low = [];",
        "161": "\t//Cap_high = [];",
        "162": "\tif (nc == 1) {",
        "163": "\t\tcost = 0;",
        "164": "\t\ttotal_cost = cost*sys_cap;",
        "165": "\t\tvalue(\"capex_table_cost_per_w\", cost);",
