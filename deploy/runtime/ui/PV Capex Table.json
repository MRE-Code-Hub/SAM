        "170": "\t//property('capex_table_inputs','ShowColumn','Capacity (kW),Costs ($/kW)');",
        "171": "\tif (sys_cap > input[nr - 1][0]) {",
        "172": "\t\tcost = input[nr - 1][1];",
        "173": "\t\tcap_string = to_string(to_int(sys_cap));",
        "174": "\t\ttable_string = to_string(to_int(input[nr-1][0]));",
        "175": "\t\tproperty('capex_dc_warning_message','Caption','The DC system capacity is '+cap_string+' Wdc and the maximum costs table DC capacity value is '+table_string + ' Wdc. The system capacity is outside the bounds of the costs table.');",
        "176": "\t}",
        "177": "\telse if (sys_cap < input[0][0]) {",
        "178": "\t\tcost = input[0][1];",
        "179": "\t\tcap_string = to_string(to_int(sys_cap));",
        "180": "\t\ttable_string = to_string(to_int(input[0][0]));",
        "181": "\t\tproperty('capex_dc_warning_message','Caption','The DC system capacity is '+cap_string+' Wdc and the minimum costs interpolation table DC capacity value is '+table_string + ' Wdc. The system capacity is outside the bounds of the costs table.');",
        "182": "\t\t\t",
        "183": "\t}",
        "184": "\telse {",
        "185": "\t\tidx = 0;",
