        "201": "\t\t\tcost_high = input[1][1];",
        "202": "\t\t}",
        "203": "\t\tcost = (sys_cap - Cap_low)/(Cap_high - Cap_low) * (cost_high - cost_low) + cost_low;",
        "204": "\t\tproperty('capex_dc_warning_message','Caption','');",
        "205": "",
        "206": "\t}",
        "207": "\ttotal_cost = cost*sys_cap;",
        "208": "\tvalue(\"capex_table_cost_per_w\", cost);",
        "209": "\tvalue(\"capex_table_total_installed_dc_cost\",total_cost);",
        "210": "\t//value(\"total_installed_cost\", total_cost);",
        "211": "\trefresh();",
        "212": "\t",
        "213": "\t/*",
        "214": "\telse if (nc == 3) {",
